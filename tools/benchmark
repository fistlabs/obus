#!/usr/bin/env node
'use strict';

var Benchmark = require('benchmark').Benchmark;
var Suite = Benchmark.Suite;
var Obus = require('../');
var suite = new Suite();

Benchmark.options.minSamples = 100;

suite.add('Obus._parse', function () {
    Obus._parse('a.b.c.d');
    Obus._parse('a["b"].c.d');
    Obus._parse('a["b"][1].d');
    Obus._parse('a["b"][1].d[1][\'2\']');
});

suite.add('Obus.parse', function () {
    Obus.parse('a.b.c.d');
    Obus.parse('a["b"].c.d');
    Obus.parse('a["b"][1].d');
    Obus.parse('a["b"][1].d[1][\'2\']');
});

suite.add('Obus._add', function () {
    var obj = {};
    Obus._add(obj, ['_', 'a', 'b', 'c', 'x', 'e'], 42);
    Obus._add(obj, ['_', 'a', 'b', 'c', 'y', 'e'], 42);
    Obus._add(obj, ['_', 'a', 'b', 'c', 'z', 'e'], 42);
});

suite.add('Obus.add', function () {
    var obj = {};
    Obus.add(obj, '_.a.b.c.x.e', 42);
    Obus.add(obj, '_.a.b.c.y.e', 42);
    Obus.add(obj, '_.a.b.c.z.e', 42);
});

suite.add('Obus._del', function () {
    var obj = {_: {a: {b: {}}}};
    Obus.del(obj, ['_', 'a', 'b' ,'c']);
    Obus.del(obj, ['_', 'a', 'b']);
    Obus.del(obj, ['_', 'a']);
});

suite.add('Obus.del', function () {
    var obj = {_: {a: {b: {}}}};
    Obus.del(obj, '_.a.b.c');
    Obus.del(obj, '_.a.b');
    Obus.del(obj, '_.a');
});

suite.add('Obus._get', function () {
    var obj = {a: {b: {c: {d: {}}}}};
    Obus._get(obj, ['a', 'b', 'c', 'd'], 4);
    Obus._get(obj, ['a', 'b', 'c', 'd', 'e'], 5);
    Obus._get(obj, ['a', 'b', 'c', 'd', 'e', 'f'], 6);
});

suite.add('Obus.get', function () {
    var obj = {a: {b: {c: {d: {}}}}};
    Obus.get(obj, 'a.b.c.d');
    Obus.get(obj, 'a.b.c.d.e');
    Obus.get(obj, 'a.b.c.d.e.f');
});

suite.add('Obus._has', function () {
    var obj = {a: {b: {c: {d: {}}}}};
    Obus._has(obj, ['a', 'b', 'c', 'd', 'e']);
    Obus._has(obj, ['a', 'b', 'c', 'd', 'e', 'f']);
    Obus._has(obj, ['a', 'b', 'c', 'd', 'e', 'f', 'g']);
});

suite.add('Obus.has', function () {
    var obj = {a: {b: {c: {d: {}}}}};
    Obus.has(obj, 'a.b.c.d.e');
    Obus.has(obj, 'a.b.c.d.e.f');
    Obus.has(obj, 'a.b.c.d.e.f.g');
});

suite.add('Obus._set', function () {
    var obj = {};
    Obus._set(obj, ['a', 'b', 'c', 'x', 'e'], 42);
    Obus._set(obj, ['a', 'b', 'c', 'y', 'e'], 42);
    Obus._set(obj, ['a', 'b', 'c', 'z', 'e'], 42);
});

suite.add('Obus.set', function () {
    var obj = {};
    Obus.set(obj, 'a.b.c.x.e', 42);
    Obus.set(obj, 'a.b.c.y.e', 42);
    Obus.set(obj, 'a.b.c.z.e', 42);
});

suite.on('cycle', function (event) {
    console.log(String(event.target));
});

suite.on('complete', function () {
    console.log();
});

suite.run({
    queued: true,
    async: true
});
