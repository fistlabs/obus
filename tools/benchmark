#!/usr/bin/env node
'use strict';

var Benchmark = require('benchmark').Benchmark;
var Suite = Benchmark.Suite;
var Obus = require('../');
var suite = new Suite();

Benchmark.options.minSamples = 100;

suite.add('Obus.get', function () {
    global.__test__ = Obus.get({a: {b: {c: {d: {}}}}}, 'a.b.c.d');
    global.__test__ = Obus.get({a: {b: {c: {d: {}}}}}, 'a.b.c.d.e');
    global.__test__ = Obus.get({a: {b: {c: {d: {}}}}}, 'a.b.c.d.e.f');
});

suite.add('Obus.has', function () {
    global.__test__ = Obus.has({a: {b: {c: {d: {}}}}}, 'a.b.c.d.e');
    global.__test__ = Obus.has({a: {b: {c: {d: {}}}}}, 'a.b.c.d.e.f');
    global.__test__ = Obus.has({a: {b: {c: {d: {}}}}}, 'a.b.c.d.e.f.g');
});

suite.add('Obus.set', function () {
    var o = {};
    global.__test__ = Obus.set(o, 'a.b.c.x.e', 42);
    global.__test__ = Obus.set(o, 'a.b.c.y.e', 42);
    global.__test__ = Obus.set(o, 'a.b.c.z.e', 42);
});

suite.add('Obus.add', function () {
    var o = {};
    global.__test__ = Obus.add(o, '_.a.b.c.x.e', 42);
    global.__test__ = Obus.add(o, '_.a.b.c.y.e', 42);
    global.__test__ = Obus.add(o, '_.a.b.c.z.e', 42);
});

suite.on('cycle', function (event) {
    console.log(String(event.target));
});

suite.on('complete', function () {
    console.log();
});

suite.run({
    queued: true,
    async: true
});
